@model IEnumerable<ESchool.Models.CourseTimeTable.TimeTable>

@{
    ViewData["Title"] = "Расписания";
}

<h1>Расписания</h1>

<p>
    <a asp-action="Create">Создать новое расписание</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @*@Html.DisplayNameFor(model => model.CourseName)*@Название курса
            </th>
            <th>
                @*@Html.DisplayNameFor(model => model.TeacherId)*@Преподаватель
            </th>
            <th>
                @*@Html.DisplayNameFor(model => model.TeacherId)*@Редактировать
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CourseName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TeacherId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a> |
                <a asp-action="CheckTT" asp-route-id="@item.Id">Entries</a> |
                <a asp-action="AddEntry" asp-route-id="@item.Id">Add entry</a> |
            </td>
        </tr>
}
    </tbody>
</table>
<div>
    <a asp-controller="Home" asp-action="Index">Назад</a>
</div>